<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.Group?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import ar.edu.itba.ati.views.PictureView?>

<fx:root type="PictureView" xmlns:fx="http://javafx.com/fxml"
         fx:controller="ar.edu.itba.ati.controller.PictureController"
         prefHeight="400.0" prefWidth="600.0">

    <HBox fx:id="imageBox" prefHeight="100.0" prefWidth="200.0">
        <children>
            <Group fx:id="imageLayer">
                <children>
                    <ImageView fx:id="imageView" fitHeight="325.0" fitWidth="510.0" pickOnBounds="true" preserveRatio="true" />
                </children>
            </Group>
        </children>
    </HBox>

</fx:root>
